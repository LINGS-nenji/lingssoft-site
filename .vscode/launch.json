{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch CRA",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3002",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "npm: start",
      "breakOnLoad": true
    }
    ,
    {
      "name": "Launch Vite",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:5173/lingssoft-site/",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "npm: dev:vite",
      "breakOnLoad": true,
      "userDataDir": "${workspaceFolder}/.vscode/vite-chrome-profile"
    }
  ]
}
